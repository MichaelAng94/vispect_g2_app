                android:layout_marginTop="@dimen/y50">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="@dimen/x40"
                        android:layout_height="@dimen/y40"
